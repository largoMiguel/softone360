{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::softone360-frontend-useast1/*",
      "Condition": {
        "StringEquals": {
          "aws:PrincipalOrgID": ""
        }
      }
    },
    {
      "Sid": "PublicReadGetObjectAnonymous",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::softone360-frontend-useast1/*"
    }
  ]
}
